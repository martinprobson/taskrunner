<?xml version="1.0" encoding="UTF-8"?>


<!--
  user_password.xml Configuration -
  This file contains the userid and password that will be used to 
  	a) Obtain a valid Kerberos ticket (if required). and/or
  	b) Log-on to cluster via JDBC.
  	
  	It is assumed this file is protected and kept in user's home directory.
  	
-->
<configuration>
    <kerberos>
        <username>
            vagrant
        </username>
        <password>
            test_password
        </password>
    </kerberos>
    <jdbc>
        <username>
            vagrant
        </username>
        <password>
            test_password
        </password>
    </jdbc>
</configuration>
